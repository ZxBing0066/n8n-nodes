{
	"name": "@heycool/n8n-nodes",
	"version": "0.1.0",
	"description": "A collection of n8n nodes.",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "https://github.com/ZxBing0066/n8n-nodes",
	"author": {
		"name": "ZxBing0066",
		"email": "ZxBing0066@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ZxBing0066/n8n-nodes.git"
	},
	"engines": {
		"node": ">=20.15"
	},
	"main": "index.js",
	"scripts": {
		"build": "npx rimraf dist && tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/ExampleCredentialsApi.credentials.js",
			"dist/credentials/HttpBinApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/ExampleNode/ExampleNode.node.js",
			"dist/nodes/HttpBin/HttpBin.node.js",
      "dist/nodes/LmChatGoogleGeminiPlus/LmChatGoogleGeminiPlus.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "~8.32.0",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.0",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"typescript": "^5.8.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"@google/genai": "^1.6.0",
		"@langchain/core": "^0.3.61"
	}
}
